javascript:var documentUrl=document.URL;if(null!==document.querySelector('link[rel="canonical"]'))var documentCanonical=document.querySelector('link[rel="canonical"]').href;else var documentCanonical='<span style="color:#f00;">Canonical nicht gesetzt!</span>';if(null!==document.querySelector('meta[name="description"]'))var snippet=document.querySelector('meta[name="description"]').content,snippetLength=snippet.length;else var snippet='<span style="color:#f00;">Meta-Description nicht gesetzt!</span>',snippetLength="n.v.";if(null!==document.querySelector("title"))var title=document.querySelector("title").textContent,titleLength=title.length;else var title='<span style="color:#f00;">Title nicht gesetzt!</span>',titleLength="n.v.";if(null!==document.querySelector('meta[name="robots"]'))var robots=document.querySelector('meta[name="robots"]').content;else var robots='<span style="color:#f00;">Meta-Robots nicht gesetzt!</span>';if(document.querySelectorAll("h1, h2, h3, h4, h5, h6").length>0){var t=document.querySelectorAll("h1, h2, h3, h4, h5, h6"),headingList=[];t.forEach(function(t){headingList.push({node:t.nodeName,text:t.textContent,classes:Array.from(t.classList).join(" ")})})}else var headingList=[];var newWindow=window.open("","seoWindow","width=768, height=700");newWindow.document.write('<html><head><link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">'),newWindow.document.write('</head><body ><div class="w3-padding-large" style="width:90%;margin:auto;"><h1>\uD83D\uDD0E SEO Info</h1>'),newWindow.document.write('<table class="w3-table-all w3-card-2 w3-hoverable w3-small" style="table-layout: fixed;overflow-wrap: break-word;"><thead><tr class="w3-blue"><th>Index-Information</th><th>Wert</th></thead><tbody><tr><td>URL</td><td>'+documentUrl+"</td></tr><tr><td>Canonical</td><td>"+documentCanonical+"</td></tr><tr><td>Robots</td><td>"+robots+"</td></tr></tbody></table><br>"),newWindow.document.write('<table class="w3-table-all w3-card-2 w3-hoverable w3-